{
  "name": "web-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "incrementVersion": "npm version --no-git-tag-version $(calver --prev $(git tag -l \"v*.*.*-[0-9]*\" --sort -version:refname | head -n 1) --silent)",
    "incrementBranchVersion": "npm version --no-git-tag-version $(calver --prefix \"${BRANCH}.\" --prev $(git tag -l \"v*-${BRANCH}.*\" --sort -version:refname | head -n 1) --silent)"
  },
  "dependencies": {
    "react": "^18",
    "react-dom": "^18",
    "next": "14.2.3"
  },
  "devDependencies": {
    "typescript": "^5",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.3"
  }
}
